<div class="container">
  <h2>Users</h2>
  <ul class="list-group">
    <% @users.each do |user| %>
      <%= link_to user_path(user) do %>
        <li class="list-group-item">
          <%= user.first_name %> <%= user.last_name %>
        </li>
      <% end %>
    <% end %>
  </ul>
  <br>

  <%= link_to 'Go back to admin panel', admin_path, class: 'btn btn-secondary' %>
</div>