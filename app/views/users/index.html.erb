<div class="container">
  <h2>Users</h2>
  <ul class="list-group">
    <% @users.each do |user| %>
        <li class="list-group-item">
          <%= link_to "#{user.first_name} #{user.last_name}", user_path(user) %>
        </li>
    <% end %>
  </ul>
  <br>

  <%= link_to 'Go back to admin panel', admin_path, class: 'btn btn-secondary' %>
</div>